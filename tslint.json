{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/**/**/*.tsx",
      "src/**/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.ts"

    ],
    "rules": {
      "member-access": true,
      "origin-ordered-imports": [ false ]
    }

  }
}
